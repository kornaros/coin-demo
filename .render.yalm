services:
  - type: web
    name: coin-demo
    env: python
    plan: free
    buildCommand: pip install -r requirements.txt
    startCommand: voila coin_game.ipynb --port=$PORT --no-browser
    envVars:
      - key: PYTHON_VERSION
        value: 3.10
      - key: VOILA_SERVER_ADDRESS
        value: 0.0.0.0
